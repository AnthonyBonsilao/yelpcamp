<% layout('./layouts/boilerplate') -%>
    <h1>Campgrounds</h1>
    <a href="/campgrounds/create">Create Campground</a>
    <ul>
        <% for (let campground of campgrounds) { %>
            <li><a href="/campgrounds/<%= campground._id %>">
                    <%= campground.title %>
                </a></li>
            <% } %>
    </ul>